{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/index.js",
    "entities:pull": "node scripts/entities-pull.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "openapi:generate": "openapi-generator-cli generate -i docs/openapi.yaml -g typescript-axios -o ../client/src/config/api",
    "typeorm": "npx tsx ./node_modules/typeorm/cli",
    "typeorm:run-migrations": "npm run typeorm -- -d ./src/db/dataSource.ts migration:run",
    "typeorm:generate-migration": "npm run typeorm -- -d ./src/db/dataSource.ts migration:generate ./src/db/migrations/$npm_config_name",
    "typeorm:create-migration": "npm run typeorm -- migration:create ./src/db/migrations/$npm_config_name",
    "typeorm:revert-migration": "npm run typeorm -- -d ./src/db/dataSource.ts migration:revert"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "morgan": "^1.10.1",
    "pg": "^8.16.3",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.27"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/morgan": "^1.9.7",
    "@types/node": "^22.18.13",
    "nodemon": "^3.1.10",
    "typeorm-model-generator": "^0.4.6",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.6.3"
  }
}